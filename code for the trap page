    <script>
        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                document.querySelector(this.getAttribute('href')).scrollIntoView({
                    behavior: 'smooth'
                });
            });
        });

        // Form submission handling
        document.querySelector('.cta-form button').addEventListener('click', function() {
            const form = this.closest('.cta-form');
            const inputs = form.querySelectorAll('input, select');
            let valid = true;

            inputs.forEach(input => {
                if (!input.value) {
                    valid = false;
                    input.style.border = '2px solid red';
                } else {
                    input.style.border = 'none';
                }
            });

            if (valid) {
                alert('–°–ø–∞—Å–∏–±–æ –∑–∞ –∑–∞—è–≤–∫—É! –ú—ã —Å–≤—è–∂–µ–º—Å—è —Å –≤–∞–º–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ 15 –º–∏–Ω—É—Ç.');
                inputs.forEach(input => input.value = '');
                
                // –ê–Ω–∞–ª–∏—Ç–∏–∫–∞: —Ñ–∏–∫—Å–∏—Ä—É–µ–º –∫–æ–Ω–≤–µ—Ä—Å–∏—é
                trackConversion();
            } else {
                alert('–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –∑–∞–ø–æ–ª–Ω–∏—Ç–µ –≤—Å–µ –ø–æ–ª—è —Ñ–æ—Ä–º—ã.');
            }
        });

        // FAQ accordion functionality
        document.querySelectorAll('.faq-item').forEach(item => {
            item.addEventListener('click', function() {
                this.classList.toggle('active');
            });
        });

        // Add animation to tooltips on scroll
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        // Observe feature cards for animation
        document.querySelectorAll('.feature-card').forEach(card => {
            card.style.opacity = '0';
            card.style.transform = 'translateY(20px)';
            card.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
            observer.observe(card);
        });

        // –ê–Ω–∞–ª–∏—Ç–∏–∫–∞: –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –≤—Ä–µ–º–µ–Ω–∏ –Ω–∞ —Å–∞–π—Ç–µ
        let timeOnSite = 0;
        const analyticsPanel = document.getElementById('analyticsPanel');
        const analyticsData = document.getElementById('analyticsData');
        
        setInterval(() => {
            timeOnSite++;
            updateAnalytics();
        }, 1000);
        
        // –ê–Ω–∞–ª–∏—Ç–∏–∫–∞: –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –∫–ª–∏–∫–æ–≤
        document.addEventListener('click', function(e) {
            if (e.target.classList.contains('btn-primary') || 
                e.target.closest('.btn-primary')) {
                trackEvent('primary_button_click');
            }
            
            if (e.target.classList.contains('tooltip')) {
                trackEvent('tooltip_hover');
            }
        });
        
        // –ê–Ω–∞–ª–∏—Ç–∏–∫–∞: –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ —Å–∫—Ä–æ–ª–ª–∞
        let maxScroll = 0;
        window.addEventListener('scroll', function() {
            const currentScroll = window.pageYOffset;
            const pageHeight = document.documentElement.scrollHeight - window.innerHeight;
            const scrollPercentage = Math.round((currentScroll / pageHeight) * 100);
            
            if (scrollPercentage > maxScroll) {
                maxScroll = scrollPercentage;
                trackEvent('scroll_progress', {percentage: maxScroll});
            }
        });
        
        // –§—É–Ω–∫—Ü–∏–∏ –∞–Ω–∞–ª–∏—Ç–∏–∫–∏
        function updateAnalytics() {
            const visitorsToday = Math.floor(Math.random() * 10) + 1;
            const conversionRate = Math.floor(Math.random() * 5) + 1;
            
            analyticsData.textContent = `–ü–æ—Å–µ—Ç–∏—Ç–µ–ª–µ–π —Å–µ–≥–æ–¥–Ω—è: ${visitorsToday} | –ö–æ–Ω–≤–µ—Ä—Å–∏—è: ${conversionRate}% | –í—Ä–µ–º—è –Ω–∞ —Å–∞–π—Ç–µ: ${timeOnSite} —Å–µ–∫`;
        }
        
        function trackEvent(eventName, data = {}) {
            // –í —Ä–µ–∞–ª—å–Ω–æ–º –ø—Ä–æ–µ–∫—Ç–µ –∑–¥–µ—Å—å –±—É–¥–µ—Ç –æ—Ç–ø—Ä–∞–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö –≤ –∞–Ω–∞–ª–∏—Ç–∏—á–µ—Å–∫—É—é —Å–∏—Å—Ç–µ–º—É
            console.log(`Event: ${eventName}`, data);
        }
        
        function trackConversion() {
            trackEvent('conversion', {type: 'demo_request'});
            
            // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø–∞–Ω–µ–ª—å –∞–Ω–∞–ª–∏—Ç–∏–∫–∏ –ø–æ—Å–ª–µ –∫–æ–Ω–≤–µ—Ä—Å–∏–∏
            analyticsPanel.style.display = 'block';
            setTimeout(() => {
                analyticsPanel.style.display = 'none';
            }, 5000);
        }
        
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∞–Ω–∞–ª–∏—Ç–∏–∫–∏
        updateAnalytics();
    </script>
</body>
</html>
